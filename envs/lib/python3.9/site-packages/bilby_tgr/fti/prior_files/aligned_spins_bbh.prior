# (Adapted from bilby)
# These are the default priors we use for BBH systems with FTI parameters.
# Note that you may wish to use more specific mass and distance parameters.
# These commands are all known to bilby.gw.prior.
# Lines beginning "#" are ignored.
mass_1 = Constraint(name='mass_1', minimum=5, maximum=100)
mass_2 = Constraint(name='mass_2', minimum=5, maximum=100)
mass_ratio = bilby.gw.prior.UniformInComponentsMassRatio(name='mass_ratio', minimum=0.125, maximum=1)
chirp_mass = bilby.gw.prior.UniformInComponentsChirpMass(name='chirp_mass', minimum=25, maximum=100)
luminosity_distance = bilby.gw.prior.UniformSourceFrame(name='luminosity_distance', minimum=1e2, maximum=5e3)
dec = Cosine(name='dec')
ra = Uniform(name='ra', minimum=0, maximum=2 * np.pi, boundary='periodic')
theta_jn = Sine(name='theta_jn')
psi = Uniform(name='psi', minimum=0, maximum=np.pi, boundary='periodic')
phase = Uniform(name='phase', minimum=0, maximum=2 * np.pi, boundary='periodic')
chi_1 = bilby.gw.prior.AlignedSpin(name='chi_1', a_prior=Uniform(minimum=0, maximum=0.99))
chi_2 = bilby.gw.prior.AlignedSpin(name='chi_2', a_prior=Uniform(minimum=0, maximum=0.99))
dchi_Minus2 = DeltaFunction(name='dchi_Minus2', peak=0)
dchi_0 = DeltaFunction(name='dchi_0', peak=0)
dchi_1 = DeltaFunction(name='dchi_1', peak=0)
dchi_2 = DeltaFunction(name='dchi_2', peak=0)
dchi_3 = DeltaFunction(name='dchi_3', peak=0)
dchi_3S = DeltaFunction(name='dchi_3S', peak=0)
dchi_3NS = DeltaFunction(name='dchi_3NS', peak=0)
dchi_4 = DeltaFunction(name='dchi_4', peak=0)
dchi_4S = DeltaFunction(name='dchi_4S', peak=0)
dchi_4NS = DeltaFunction(name='dchi_4NS', peak=0)
dchi_5l = DeltaFunction(name='dchi_5l', peak=0)
dchi_5lS = DeltaFunction(name='dchi_5lS', peak=0)
dchi_5lNS = DeltaFunction(name='dchi_5lNS', peak=0)
dchi_6 = DeltaFunction(name='dchi_6', peak=0)
dchi_6S = DeltaFunction(name='dchi_6S', peak=0)
dchi_6NS = DeltaFunction(name='dchi_6NS', peak=0)
dchi_6l = DeltaFunction(name='dchi_6l', peak=0)
dchi_7 = DeltaFunction(name='dchi_7', peak=0)
dchi_7S = DeltaFunction(name='dchi_7S', peak=0)
dchi_7NS = DeltaFunction(name='dchi_7NS', peak=0)
dchi_kappaS = DeltaFunction(name='dchi_kappaS', peak=0)
dchi_kappaA = DeltaFunction(name='dchi_kappaA', peak=0)
f_window_div_f_peak = DeltaFunction(name='f_window_div_f_peak', peak=1.0)
NumCycles = DeltaFunction(name='NumCycles', peak=1)
